<!--page/one/index.wxml-->
<view class="page">
    <view class="page-bottom">
        <view class="page-content">
            <view  class="wc">
                <navigator url="../card/card" class="weui-cell weui-cell_access">
                    <view class="weui-cell__bd">科大记忆</view>
                    <view class="weui-cell__ft weui-cell__ft_in-access"></view>
                </navigator>
            </view>
            <view  class="wc">
                <navigator url="../picwall/picwall" class="weui-cell weui-cell_access">
                    <view class="weui-cell__bd">60周年照片墙</view>
                    <view class="weui-cell__ft weui-cell__ft_in-access"></view>
                </navigator>
            </view>
            <view  class="wc">
                <navigator url="../user/index" class="weui-cell weui-cell_access">
                    <view class="weui-cell__bd">个人中心</view>
                    <view class="weui-cell__ft weui-cell__ft_in-access"></view>
                </navigator>
            </view>
        </view>
    </view>
    <view bindtouchmove="tap_drag" bindtouchend="tap_end" bindtouchstart="tap_start" class="page-top {{open ? 'c-state2' : ''}}">
        <image bindtap="tap_ch" src="../../img/btn.png"></image>
    </view>
</view>
